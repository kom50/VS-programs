//Exception Propagation

public class ExecptionDemo3 {
    void m() {
    	int m1 = 5 / 0;
    }
    
    void p() {
    	m();
    }
    
    void n() {
    	p();
    }
    public static void main(String []args) {
    	ExceptionDemo1 e1 = new ExceptionDemo1();
    	e1.n();
    	System.out.println("Rest of the code");
    }
}
